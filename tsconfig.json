{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "preserveWatchOutput": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "types": []
  }
}